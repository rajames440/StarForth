: STABLE-PHASE ( -- )
  20 0 DO
    10 0 DO I J + DROP LOOP
  LOOP ;

: DIVERSE-PHASE ( -- )
  20 0 DO
    I 3 MOD CASE
      0 OF 42 DUP * DROP ENDOF
      1 OF 1 50 DO I LOOP DROP ENDOF
    ENDCASE
  LOOP ;

